<h1>  New Consumer</h1>



<%= form_for @consumer, url:"/create_consumer" do |form|%>
<div>
	<%= form.label :name %><br>
	<%= form.text_field :name%><br> 
</div>
<div>
	<%= form.label :address%><br>
	<%= form.text_area :address%><br>
</div>

<div>
	<%= form.submit "save" %><br>
</div>

<% end %>